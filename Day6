#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int t;
    cin >> t;
  
    for(int k = 0 ; k < t; k++){
        string str;
        cin >> str;
        string even;
        string odd;
        for(int i = 0; i < str.length(); i++){
            
            if(i % 2 == 0){
                even += str[i];
            }
            if(i % 2 != 0){
                odd += str[i];
            }
        }
        
        cout << even << " " << odd << endl;
    }
    
    
    return 0;
}
